{namespace pokemon}

/**
 * Prints a pokemon from the list.
 * @param? pokemon
 * @param? selectPokemon
 */
{template .list_view}
<div data-onclick="{$selectPokemon}" data-pokemon="{$pokemon}">
	{$pokemon.name}
	{$pokemon.order}
	{$pokemon.type}
	{$pokemon.frontImageURL}
	{$pokemon.frontShinyImageURL}
	{$pokemon.backImageURL}
	{$pokemon.backShinyImageURL}
</div>
{/template}